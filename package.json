{
  "name": "@microfleet/amqp",
  "private": true,
  "description": "AMQP driver for node",
  "keywords": [
    "amqp",
    "rabbitmq"
  ],
  "workspaces": [
    "packages/*"
  ],
  "version": "1.3.1",
  "author": {
    "name": "David Barshow"
  },
  "contributors": [
    "Vitaly Aminev <v@makeomatic.ca>"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/dropbox/amqp-coffee/master/LICENSE.txt"
    }
  ],
  "main": "./bin/src/amqp.js",
  "scripts": {
    "semantic-release": "semantic-release",
    "jest": "lerna run test --stream",
    "test": "yarn lint && yarn jest",
    "lint": "lerna run lint --stream",
    "prepare": "lerna run prepare",
    "prebuild": "yarn clean",
    "build": "tsc -b packages/*/tsconfig.build.json",
    "clean": "rimraf packages/*/lib",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "preparecommitmsg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $GIT_PARAMS"
