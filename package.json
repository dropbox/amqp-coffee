    "lodash": "^4.17.10",
    "lodash.defer": "^4.1.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/hapi__joi": "^15.0.0",
    "@types/lodash.defer": "^4.1.3",
    "@types/node": "^11.13.7",
    "coffeescript": "^2.4.1",
    "istanbul": "0.4.5",
    "mocha": "^6.1.4",
    "should": "13.2.3",
    "typescript": "^3.4.5",
    "underscore": "^1.9.1",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dropbox/amqp-coffee.git"
  },
  "engines": {
    "node": ">=8.9.0"
